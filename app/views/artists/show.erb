<h1><%= @artist.name %></h1></br>
<h3>Genres:</h3></br>
<ol>
<% @artist.genres.each do |g| %>
    <li><a href="/genres/<%= g.slug %>"><h3><%= g.name %></h3></a></li>
    <% end %>
<h3>Songs:</h3></br>
</ol>
<% @artist.songs.each do |s| %>
    <li><a href="/songs/<%= s.slug %>"><h3><%= s.name %></h3></a></li>
    <% end %>
</ol>

